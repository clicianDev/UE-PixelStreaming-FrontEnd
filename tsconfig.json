{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "module": "esnext",
    "esModuleInterop": true,
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "allowJs": true,
    "declaration": false,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "*": ["*", "src/*"]
    }
  },
  "lib": ["es2015", "dom", "esnext"],
  "include": ["./src/*.tsx"],
}
